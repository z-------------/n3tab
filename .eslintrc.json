{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "sourceType": "module"
    },
    "plugins": ["@typescript-eslint"],
    "extends": "eslint:recommended",
    "env": {
        "browser": true
    },
    "rules": {
        "indent": ["error", 4],
        "quotes": ["error", "double"]
    }
  }
